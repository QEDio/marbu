%div{:id => 'root', :data => {:role => 'page'}}
  = haml :header, :layout => false
  %div{:data => {:role => 'content'}}
    %ul{:data => {:role => 'listview'}}
      %a{:href => 'builder/new', :data => {:transition => 'slide', :role => 'button'}}= "new"
      - @mrfs.each do |mrf|
        - uuid = mrf.uuid
        - name = mrf.name
        %li
          %a{:href => 'builder/' + uuid, :data => {:transition => 'slide'}}=name.capitalize

  = haml :footer, :layout => false